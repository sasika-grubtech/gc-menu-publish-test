{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2022",
    "declaration": true,
    "outDir": "dist",
    "strict": true,
    "allowJs": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "types": ["cypress", "cypress-xpath","node", "@cypress/grep"],
    "skipLibCheck": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "baseUrl": "./",
    "paths": {
      "cypress/*": ["./cypress/*"]
    }
  },
  "include": ["cypress/**/*.ts", "**/*.ts"],
  "exclude": ["node_modules", "dist"],
  "typedocOptions": {
    "entryPoints": ["index.ts"],
    "out": "docs"
  }
}
